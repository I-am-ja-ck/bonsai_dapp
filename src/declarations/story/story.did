type StoryText = 
 record {
   address: opt text;
   story: text;
   title: text;
 };
type StoryReturn = 
 record {
   author: principal;
   story: StoryText;
   storyId: nat;
   totalVotes: nat;
 };
type Result_2 = 
 variant {
   err: text;
   ok: text;
 };
type Result_1 = 
 variant {
   err: text;
   ok: StoryReturn;
 };
type Result = 
 variant {
   err: text;
   ok: vec nat;
 };
service : {
  add: (StoryText) -> (Result_2);
  adminDelete: (nat) -> (Result_2);
  delete: (nat) -> (Result_2);
  get: (nat) -> (Result_1) query;
  getDebug: () -> (reserved) query;
  getMemorySize: () -> (nat) query;
  getStoryIds: (nat) -> (Result) query;
  getUserStories: (text) -> (Result) query;
  whoami: () -> (principal);
}
